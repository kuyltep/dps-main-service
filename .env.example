# Database
POSTGRES_USER=dps-user
POSTGRES_PASSWORD=dps-password
POSTGRES_DB=main
DATABASE_URL=postgresql://dps-user:dps-password@postgres:5432/main

# MinIO
MINIO_ROOT_USER=dps_login
MINIO_ROOT_PASSWORD=dps_password
MINIO_ENDPOINT=minio
MINIO_PORT=9000

# Qdrant
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# Service ports (updated according to current configuration)
